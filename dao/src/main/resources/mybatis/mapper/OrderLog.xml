<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.wanxi.mall.dao.OrderLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cn.wanxi.mall.bean.OrderLog">
        <id column="id" property="id" />
        <result column="operater" property="operater" />
        <result column="operate_time" property="operateTime" />
        <result column="order_id" property="orderId" />
        <result column="order_status" property="orderStatus" />
        <result column="pay_status" property="payStatus" />
        <result column="consign_status" property="consignStatus" />
        <result column="remarks" property="remarks" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, operater, operate_time, order_id, order_status, pay_status, consign_status, remarks
    </sql>

</mapper>
