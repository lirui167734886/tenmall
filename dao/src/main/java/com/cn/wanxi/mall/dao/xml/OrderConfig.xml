    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cn.wanxi.mall.bean.OrderConfig">
        <id column="id" property="id" />
        <result column="order_timeout" property="orderTimeout" />
        <result column="seckill_timeout" property="seckillTimeout" />
